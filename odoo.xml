    <variable name="rule_name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="res_model" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="odoo_notebook" value="&lt;notebook&gt;&#10;    &lt;page string=&quot;$string$&quot;&gt;&#10;        &lt;group&gt;&#10;            &lt;field name=&quot;$field_name$&quot;/&gt;&#10;        &lt;/group&gt;&#10;    &lt;/page&gt;&#10;&lt;/notebook&gt;" description="Add Notebook and a page" toReformat="true" toShortenFQNames="true">
