  <template name="odoo_group_by" value="&lt;record id=&quot;view_$res_id$_search&quot; model=&quot;ir.ui.view&quot;&gt;&#10;   &lt;field name=&quot;name&quot;&gt;view.$res_model$.search&lt;/field&gt;&#10;   &lt;field name=&quot;model&quot;&gt;$res_model$&lt;/field&gt;&#10;    &lt;field name=&quot;arch&quot; type=&quot;xml&quot;&gt;&#10;        &lt;search string=&quot;$string$&quot;&gt;&#10;            &lt;group expand=&quot;1&quot; string=&quot;Group By&quot;&gt;&#10;                &lt;filter string=&quot;$string2$&quot;&#10;                        name=&quot;$name$&quot;&#10;                        context=&quot;{'$ctx1$': '$ctx2$'}&quot;/&gt;&#10;            &lt;/group&gt;&#10;    &lt;/search&gt;&#10;   &lt;/field&gt;&#10;&lt;/record&gt;" description="Create Search View" toReformat="true" toShortenFQNames="true">
